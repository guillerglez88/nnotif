{
  "name": "nnotif-dynarest",
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "start:dev": "npm-run-all build:dev debug",
    "dev": "nodemon --watch src -e ts,json,js --exec npm run start:dev",
    "debug": "node --inspect=0.0.0.0 ./dist/bin/www.js",
    "build": "npm-run-all clean tsc",
    "build:dev": "npm-run-all clean tsc:dev",
    "tsc": "tsc -p tsconfig.json",
    "tsc:dev": "tsc --sourceMap true -p tsconfig.json",
    "check-types": "tsc --noEmit -p tsconfig.json",
    "clean": "rimraf dist/*"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.18.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.16",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2"
  }
}